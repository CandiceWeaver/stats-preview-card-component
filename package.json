{
  "name": "stats-preview-card-component",
  "version": "1.0.0",
  "description": "![Design preview for the Stats preview card component coding challenge](./design/desktop-preview.jpg)",
  "main": "index.js",
	"scripts": {
		"browserslist": "last 10 versions",
		"watch:sass": "node-sass sass/main.scss css/style.css -w",
		"devserver": "live-server",
		"start": "npm-run-all --parallel devserver watch:sass",
		"compile:sass": "node-sass sass/main.scss css/style.comp.css",
		"concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
		"prefix:css": "postcss css/style.concat.css --use autoprefixer -o css/style.prefix.css",
		"compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
		"build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
	},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CandiceWeaver/stats-preview-card-component.git"
  },
  "author": "Candice",
  "license": "ISC",
  "devDependencies": {
		"autoprefixer": "^10.0.1",
		"concat": "^1.0.3",
		"node-sass": "^4.14.1",
		"npm-run-all": "^4.1.1",
		"postcss": "^8.1.1",
		"postcss-cli": "^8.0.0"
	},
  "bugs": {
    "url": "https://github.com/CandiceWeaver/stats-preview-card-component/issues"
  },
  "homepage": "https://github.com/CandiceWeaver/stats-preview-card-component#readme"
}
